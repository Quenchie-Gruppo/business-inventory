{
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "InventoryMS.settings.production"
    },
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  